model Menu {
  id          Int         @id @default(autoincrement())
  title       String
  photo       String
  description String
  price       Float
  status      Boolean
  SellFast    Boolean     @default(false) @map("sell-fast")
  Business    Business?   @relation(fields: [businessId], references: [id])
  businessId  Int?
  orderItem   orderItem[]
}
